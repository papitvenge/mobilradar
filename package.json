{
  "name": "mobilradar",
  "version": "1.0.0",
  "description": "Mobile radar app using Bluetooth to detect nearby devices",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "ios:device": "LANG=en_US.UTF-8 LC_ALL=en_US.UTF-8 expo run:ios --device --configuration Release",
    "ios:simulator": "expo run:ios",
    "ios:open": "open ios/MobilRadar.xcworkspace",
    "ios:build:device": "cd ios && xcodebuild -workspace MobilRadar.xcworkspace -scheme MobilRadar -configuration Release -destination 'generic/platform=iOS' -allowProvisioningUpdates build",
    "web": "expo start --web",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "echo 'Build command placeholder'"
  },
  "dependencies": {
    "expo": "^54.0.25",
    "expo-camera": "~17.0.10",
    "expo-constants": "~18.0.13",
    "expo-linking": "~8.0.11",
    "expo-location": "~19.0.8",
    "expo-router": "^6.0.23",
    "expo-status-bar": "~3.0.8",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-ble-manager": "^11.5.3",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "~0.21.0",
    "react-native-wifi-reborn": "^4.12.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "babel-preset-expo": "^54.0.7",
    "react-refresh": "^0.18.0"
  },
  "engines": {
    "node": "20.x"
  },
  "overrides": {
    "escape-string-regexp": "^4.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/papitvenge/mobilradar.git"
  },
  "keywords": [
    "bluetooth",
    "radar",
    "proximity",
    "mobile"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/papitvenge/mobilradar/issues"
  },
  "homepage": "https://github.com/papitvenge/mobilradar#readme",
  "private": true
}
